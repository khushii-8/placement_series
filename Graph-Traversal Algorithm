#include <bits/stdc++.h>
using namespace std;

void bfs(int start, vector<vector<int>>& adj, int n) {
    vector<int> vis(n, 0);
    queue<int> q;
    q.push(start);
    vis[start] = 1;

    cout << "BFS: ";
    while(!q.empty()) {
        int node = q.front(); q.pop();
        cout << node << " ";
        for(int nbr : adj[node]) {
            if(!vis[nbr]) {
                vis[nbr] = 1;
                q.push(nbr);
            }
        }
    }
    cout << "\n";
}

void dfsHelper(int node, vector<vector<int>>& adj, vector<int>& vis) {
    vis[node] = 1;
    cout << node << " ";
    for(int nbr : adj[node])
        if(!vis[nbr]) dfsHelper(nbr, adj, vis);
}

void dfs(int start, vector<vector<int>>& adj, int n) {
    vector<int> vis(n, 0);
    cout << "DFS: ";
    dfsHelper(start, adj, vis);
    cout << "\n";
}

int main() {
    int n = 5;
    vector<vector<int>> adj = {
        {1,2}, {0,3}, {0,4}, {1}, {2}
    };

    bfs(0, adj, n);
    dfs(0, adj, n);
}
