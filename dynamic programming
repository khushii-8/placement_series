#include <bits/stdc++.h>
using namespace std;

// Memoization
int fibM(int n, vector<int>&dp){
    if(n<=1) return n;
    if(dp[n]!=-1) return dp[n];
    return dp[n]=fibM(n-1,dp)+fibM(n-2,dp);
}

// Tabulation
int fibT(int n){
    vector<int>dp(n+1);
    dp[0]=0, dp[1]=1;
    for(int i=2;i<=n;i++) dp[i]=dp[i-1]+dp[i-2];
    return dp[n];
}

int main(){
    int n=6; vector<int>dp(n+1,-1);
    cout<<"Memoization: "<<fibM(n,dp)<<endl;
    cout<<"Tabulation: "<<fibT(n);
}
