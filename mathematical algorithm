#include <bits/stdc++.h>
using namespace std;
const int MOD = 1e9 + 7;

// ðŸ”¹ Eulerâ€™s Totient Function (Ï†(n))
int phi(int n) {
    int res = n;
    for (int p = 2; p * p <= n; p++) {
        if (n % p == 0) {
            while (n % p == 0) n /= p;
            res -= res / p;
        }
    }
    if (n > 1) res -= res / n;
    return res;
}

// ðŸ”¹ Modular Exponentiation
long long modExp(long long a, long long b, long long m) {
    long long res = 1;
    while (b > 0) {
        if (b & 1) res = (res * a) % m;
        a = (a * a) % m;
        b >>= 1;
    }
    return res;
}

// ðŸ”¹ Factorial (for nCr)
long long fact(int n) {
    long long res = 1;
    for (int i = 1; i <= n; i++) res = (res * i) % MOD;
    return res;
}

// ðŸ”¹ nCr = n! / (r! * (n-r)!)
long long nCr(int n, int r) {
    if (r > n) return 0;
    long long num = fact(n);
    long long den = (fact(r) * fact(n - r)) % MOD;
    return (num * modExp(den, MOD - 2, MOD)) % MOD;
}

// ðŸ”¹ Inclusion-Exclusion Principle Example:
// Count numbers â‰¤ N divisible by 2 or 3
int inclusionExclusion(int N) {
    int a = N / 2;
    int b = N / 3;
    int ab = N / 6;
    return a + b - ab;
}

// ðŸ”¹ Catalan Number (n-th)
long long catalan(int n) {
    long long res = nCr(2 * n, n);
    return (res * modExp(n + 1, MOD - 2, MOD)) % MOD;
}

int main() {
    // âœ… Euler's Totient
    cout << "Phi(10): " << phi(10) << endl;

    // âœ… nCr
    cout << "nCr(5,2): " << nCr(5, 2) << endl;

    // âœ… Inclusionâ€“Exclusion
    cout << "Count â‰¤ 10 divisible by 2 or 3: " << inclusionExclusion(10) << endl;

    // âœ… Catalan Number
    cout << "Catalan(4): " << catalan(4) << endl;

    return 0;
}
